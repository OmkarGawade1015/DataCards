<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./Partials/Headabc') %>
    <meta charset="UTF-8" />
    <title>HomePage</title>
    <meta name="viewport" content="width=device-width, initial scale=1" />
    <link rel="stylesheet" type="text/css" href="../public/styles/home.css"/>
    
</head>
<body>
    
    <%- include('./Partials/nava') %> 
   
        <div class="row row-cols-4">
        <div class="container">
         <% blogs.forEach(blog => { %>
            <div class="card-body">
               <div class="col" >
              
            
                <img src="<%= blog.coverImage %>" class="card-img-top" >
                
                  <h5 class="card-title"><%= blog.title %></h5>
                  
                  <a href="/blog/<%= blog._id %>" class="btn btn-primary">View</a>
                
             </div>
            
        </div>
        <% }) %>
    
     
    <%- include('./Partials/scripts') %>
</body>
</html>
