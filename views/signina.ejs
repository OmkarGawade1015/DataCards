<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
    <%- include('./Partials/Headabc') %>
    <title>Signin</title>
    <meta name="viewport" content="width=device-width, initial scale=1" />
    <link rel="stylesheet" type="text/css" href="../public/styles/signin.css"/>
</head>
<body>
    <%- include('./Partials/nava') %>

    <div class="container mt-4">
        <form action="/user/signina" method="post">
          

            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Email address</label>
              <input type="email" name="email" id="exampleInputEmail1" aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>
            <div class="mb-3">
              <label for="exampleInputPassword1" class="form-label">Password</label>
              <input type="password" name="password" id="exampleInputPassword1">
            </div>
            
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>
    </div>
    <%- include('./Partials/scripts') %>
</body>
</html>
